rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /settings/{document=**} {
      allow read, write: if true;
    }
    match /{document=**} {
      allow read, write: if false;
    }
  }
}

service firebase.storage {
  match /b/{bucket}/o {
    match /uploads/{allPaths=**} {
      allow read;
      allow write: if true; 
    }
  }
}
